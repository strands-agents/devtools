{
  "name": "cdk-evals",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Strands Evals Dashboard and Pipeline",
  "bin": {
    "cdk": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all",
    "deploy:dashboard": "cdk deploy DashboardStack",
    "deploy:pipeline": "cdk deploy EvalPipelineStack",
    "destroy": "cdk destroy --all"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "aws-cdk": "^2.170.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.170.0",
    "@aws-cdk/aws-lambda-python-alpha": "^2.170.0-alpha.0",
    "constructs": "^10.0.0"
  }
}
